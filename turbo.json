{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"outputs": ["dist/**", "storybook-static/**", ".output"],
			"dependsOn": ["^build"]
		},
		"preview-storybook": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"start": {
			"cache": false,
			"persistent": true,
			"outputs": [".output"]
		},
		"test": {},
		"test:coverage": {},
		"test:watch": {
			"cache": false,
			"persistent": true
		},
		"types": {},
		"clean": {
			"cache": false
		},
		"format-and-lint": {},
		"format-and-lint:fix": {
			"cache": false
		}
	}
}
