{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Solid Start",
	"extends": "./base.json",
	"compilerOptions": {
		"allowJs": true,
		"declaration": false,
		"declarationMap": false,
		"incremental": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"noEmit": true,
		"resolveJsonModule": true,
		"target": "ES2020"
	}
}
